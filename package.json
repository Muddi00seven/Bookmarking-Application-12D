    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "dayjs": "^1.9.4",
    "dotenv": "^8.2.0",
    "formik": "^2.2.1",
    "gatsby": "^2.24.91",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-typescript": "^2.4.25",
    "graphql": "^15.3.0",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.0.5",
    "yup": "^0.29.3"
  },
  "devDependencies": {
